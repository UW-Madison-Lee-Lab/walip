{
  "model": {
    "src_lang": "en",
    "tgt_lang": "it"
 },
  "words":{ 
    "word_data" : "imagenet",
    "data_mode" : "extended",
    "num_prompts": 3
  },
  "images": {
    "image_data": "imagenet",
    "num_images": 1,
    "using_filtered_images": false
  },

  "embedding": {
    "emb_type": "fp",
    "large_model": true,
    "reuse_embedding": false,
    "reuse_text_embedding": false,
    "reuse_image_embedding": false,
    "reuse_image_data": true
  },

  "train": {
    "work_mode": "translation",
    "supervised": false,
    "sim_score": "cosine",
    "matching_method": "quad_ot"
  },

  "dico": {
    "dict_pth": "../dicts/",
    "dico_eval": "default",
    "dico_method": "csls_knn_10",
    "dico_build": "S2T",
    "dico_threshold": 0,
    "dico_max_rank": 15000,
    "dico_min_size": 0,
    "dico_max_size": 0
  }

}
