{
  "model": {
    "src_lang": "en",
    "tgt_lang": "it"
 },
  "words":{ 
    "word_data" : "cifar10",
    "data_mode" : "test"
  },
  "images": {
    "image_data": "cifar10",
    "num_images": 2,
    "num_prompts": 10,
    "using_filtered_images": false
  },

  "embedding": {
    "emb_type": "fp",
    "large_model": true,
    "reuse_embedding": true,
    "reuse_text_embedding": false,
    "reuse_image_embedding": false,
    "reuse_image_data": true
  },

  "train": {
    "work_mode": "analysis",
    "supervised": false,
    "sim_score": "csls",
    "matching_method": "nn"
  },

  "dico": {
    "dict_pth": "../dicts/",
    "dico_eval": "default",
    "dico_method": "csls_knn_10",
    "dico_build": "S2T",
    "dico_threshold": 0,
    "dico_max_rank": 15000,
    "dico_min_size": 0,
    "dico_max_size": 0
  }

}
