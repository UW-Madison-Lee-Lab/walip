{
  "langs": {
    "src_lang": "en",
    "tgt_lang": "it"
 },
  "words":{ 
    "word_data" : "wiki",
    "data_mode" : "test"
  },
  "images": {
    "image_data": "imagenet",
    "num_images": 10,
    "num_prompts": 4,
    "using_filtered_images": false
  },

  "model": {
    "emb_type": "fp",
    "large_model": false,
    "sim_score": "csls",
    "row_ranking": false,
    "matching_method": "nn"
  },

  "work": {
    "work_mode": "translation",
    "method": "unsupervised",
    "reuse_embedding":1,
    "reuse_text_embedding": 0,
    "reuse_image_embedding": 0,
    "reuse_image_data": 1
  },

  "setting": {
    "dict_pth": "../dicts/",
    "seed": -1,
    "verbose": 2,
    "num_workers": 2,
    "dico_eval": "default",
    "dico_method": "csls_knn_10",
    "dico_build": "S2T",
    "dico_threshold": 0,
    "dico_max_rank": 15000,
    "dico_min_size": 0,
    "dico_max_size": 0
  }
}
